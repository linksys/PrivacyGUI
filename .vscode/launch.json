{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "linksys - Web (Remote)",
            "request": "launch",
            "type": "dart",
            "args": ["--web-browser-flag", "--disable-web-security"],
            "toolArgs": ["--dart-define","force=remote"],
            "program": "lib/main.dart"
        },
        {
            "name": "linksys - Web (Local)",
            "request": "launch",
            "type": "dart",
            "args": ["--web-browser-flag", "--disable-web-security"],
            "toolArgs": ["--dart-define","force=local"],
            "program": "lib/main.dart"
        },
        {
            "name": "linksys - Android",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart"
        },
        {
            "name": "linksys - Android (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart"

        },
        {
            "name": "linksys - Android (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart"

        },
        {
            "name": "linksys - iOS ",
            "request": "launch",
            "type": "dart",
            "args": ["--flavor", "Linksys"],
            "program": "lib/main.dart"
        },
        {
            "name": "linksys - iOS (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["--flavor", "Linksys"],
            "program": "lib/main.dart"

        },
        {
            "name": "linksys - iOS (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["--flavor", "Linksys"],
            "program": "lib/main.dart"

        },
        {
            "name": "linksys - iOS - Enterprise ",
            "request": "launch",
            "type": "dart",
            "args": ["--flavor", "Enterprise"],
            "program": "lib/main.dart"

        },
        {
            "name": "linksys - iOS - Enterprise (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["--flavor", "Enterprise"],
            "program": "lib/main.dart"

        },
        {
            "name": "linksys - iOS - Enterprise (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["--flavor", "Enterprise"],
            "program": "lib/main.dart"

        },
        {
            "name": "Linksys widget storybook",
            "cwd": "packages/linksys_widgets/example",
            "request": "launch",
            "type": "dart",
            "program": "lib/storybook.dart"
        },
    ]
}